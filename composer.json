{
    "name": "processwith/processwith-php",
    "description": "Easily switch from paystack to flutterwave(vice versa) in your PHP applications.",
    "keywords": ["processwith", "paystack", "flutterwave", "rave", "paylink", "monnify"],
    "homepage": "https://github.com/processwith/processwith-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "ProcessWith API Team",
            "email": "hello@processwith.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "php-curl-class/php-curl-class": "^8.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ProcessWith\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProcessWith\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
